(define (problem gripper-problem)
    (:domain gripper-domain)
    (:objects c0 c1 c2 p00 p01 p02 p10 p11 p12 t)
    (:init
        (cylinder c0)(cylinder c1)(cylinder c2)
        (tissue t)
        (object c0)(object c1)(object c2)(object t)
        (loc p00)(loc p01)(loc p02)
        (loc p10)(loc p11)(loc p12)
        (free)
        (at c0 p00)(at c1 p02)(at c2 p12)(at t p11)
        (pickable c0)(pickable c1)(pickable c2)(pickable t)
        (dirty p10)
    )
    (:goal (and
                (not(dirty p10))(at c1 p10)(on c0 c1)(on c2 c0)(at t p11)
            )
    )
)